{
  "name": "vue-wap",
  "version": "1.0.0",
  "description": "基于Vue的 Wap 项目",
  "author": "盘乐 <1093683149@qq.com>",
  "private": true,
  "scripts": {
    "serve": "yarn && vue-cli-service serve",
    "build": "yarn && vue-cli-service build --mode tst && node ./distCopyConfig/toTST",
    "build-rc": "yarn && vue-cli-service build --mode rc && node ./distCopyConfig/toRC",
    "build-prod": "yarn && vue-cli-service build --mode prod && node ./distCopyConfig/toPROD",
    "lint": "vue-cli-service lint"
  },
  "dependencies": {
    "axios": "^0.27.2",
    "better-scroll": "^1.15.2",
    "vant": "^2.2.0",
    "vue": "^2.6.10",
    "vue-class-component": "^6.0.0",
    "vue-count-to": "^1.0.13",
    "vue-property-decorator": "^7.0.0",
    "vue-router": "^3.0.1",
    "vue-scroller": "^2.2.4",
    "vuex": "^3.0.1",
    "vuex-module-decorators": "^0.16.1",
    "vuex-persistedstate": "^3.0.1"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "^3.5.0",
    "@vue/cli-plugin-typescript": "^3.5.0",
    "@vue/cli-service": "^3.5.0",
    "babel-plugin-component": "^1.1.1",
    "babel-plugin-import": "^1.11.0",
    "ftp": "^0.3.10",
    "less": "^3.0.4",
    "less-loader": "^4.1.0",
    "typescript": "^3.2.1",
    "vue-template-compiler": "^2.6.10"
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {}
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 8"
  ]
}
